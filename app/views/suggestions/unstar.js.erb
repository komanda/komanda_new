<% if @suggestion %>
	$("#<%= @suggestion.id %> #vote_link a").attr("href", "/suggestions/star?suggestion=<%= @suggestion.id %>")
	$("#<%= @suggestion.id %> #vote_link a img").attr("src", "/assets/star_empty.png")
	$('#vote_count_<%= @suggestion.id %>').text('<%= @suggestion.vote_counter %>')
<% end %>